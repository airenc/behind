or=\"(item, index) in list\" :key=\"index\" @click=\"add(item)\">\r\n          <img :src=\"item.imgUrl\" alt=\"\" @click=\"pop\" />\r\n          <br />\r\n          {{ item.name }}\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <my-mask :show=\"show\" @hide=\"hide\">\r\n      <ul id=\"uu\">\r\n        <p>\r\n          已选食物({{ $store.state.shopcar.length }})\r\n\r\n          <button @click=\"goto\">去测评</button>\r\n        </p>\r\n        <li v-for=\"(item, index) in $store.state.shopcar\" :key=\"index\">\r\n          {{ item.name }}\r\n\r\n          <span @click=\"del(index)\">x </span>\r\n        </li>\r\n      </ul>\r\n    </my-mask>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// 引入\r\nimport headers from \"../components/Headers.vue\";\r\nexport default {\r\n  // 注册\r\n  components: {\r\n    headers,\r\n  },\r\n  data() {\r\n    return {\r\n      // 接受数据 是三种\r\n      // 合并用的数据\r\n      listone: [],\r\n      listtwo: [],\r\n      listthree: [],\r\n      //每次展示的数据\r\n      list: [],\r\n      // 搜索数据\r\n      sos: \"\",\r\n      // 合并后的数据\r\n      lists: [],\r\n\r\n      // 显示隐藏\r\n      show: false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getone();\r\n    this.gettwo();\r\n    this.getthree();\r\n\r\n    this.$axios(\"/主食.json\").then((res) => {\r\n      this.listone = res.data;\r\n      this.list = this.listone;\r\n    });\r\n    this.$axios(\"/豆奶制品.json\").then((res) => {\r\n      this.listtwo = res.data;\r\n    });\r\n    this.$axios(\"/蔬菜类.json\").then((res) => {\r\n      console.log(this.listtwo);\r\n\r\n      this.listthree = res.data;\r\n      this.lists = [...this.listone, ...this.listtwo, ...this.listthree];\r\n    });\r\n  },\r\n  methods: {\r\n    // 点击主食\r\n    getone() {\r\n      this.list = this.listone;\r\n    },\r\n\r\n    //豆制奶品\r\n    gettwo() {\r\n      this.list = this.listtwo;\r\n    },\r\n    //点击蔬菜类\r\n    getthree() {\r\n      this.list = this.listthree;\r\n    },\r\n\r\n    //\r\n    goto() {\r\n      this.$router.push(\"/detail\");\r\n    },\r\n\r\n    //搜索\r\n    get() {\r\n      //新的数据来接受  从完整的数据里搜索\r\n      this.list = this.lists.filter((item) => {\r\n        return item.name.includes(this.sos);\r\n      });\r\n      this.sos = \"\";\r\n    },\r\n\r\n    // 添加\r\n    add(item) {\r\n      this.$store.commit(\"add\", item);\r\n    },\r\n\r\n    // 显示\r\n    pop() {\r\n      this.show = true;\r\n    },\r\n\r\n    // 隐藏\r\n    hide() {\r\n      this.show = false;\r\n    },\r\n\r\n    del(index) {\r\n      this.$store.commit(\"del\", index);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n#box {\r\n  width: 100%;\r\n  // background: pink;\r\n}\r\n#left {\r\n  width: 25%;\r\n  text-align: center;\r\n  // border: red solid;\r\n  float: left;\r\n  font-size: 16px;\r\n  li {\r\n    height: 35px;\r\n    line-height: 35px;\r\n  }\r\n}\r\n\r\n#left {\r\n  li:hover {\r\n    background: rgba(0, 0, 0, 0.4);\r\n  }\r\n}\r\n\r\n#reight {\r\n  width: 74%;\r\n  // background: burlywood;\r\n  float: left;\r\n\r\n  li {\r\n    width: 33%;\r\n    height: 120px;\r\n    float: left;\r\n    img {\r\n      margin-top: 10px;\r\n      width: 60px;\r\n      height: 60px;\r\n    }\r\n  }\r\n}\r\n\r\nimg:hover {\r\n  border: crimson 2px solid;\r\n}\r\n\r\n#uu {\r\n  width: 100%;\r\n  li {\r\n    width: 18%;\r\n    float: left;\r\n    margin-left: 2px;\r\n    border: 1px solid;\r\n  }\r\n}\r\n</style>\r\n"]}]}